{
  "name": "printaeu",
  "version": "0.1.10",
  "description": "Alternative to console.log",
  "main": "printaeu.js",
  "types": "printaeu.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planetsLightningArrester/printaeu.git"
  },
  "scripts": {
    "build": "run-script-os",
    "build:linux": "rm -rf dist && mkdir -p ./dist && node ./node_modules/typescript/bin/tsc ./src/printaeu.ts --declaration && cp -rft ./dist ./src/printaeu.js ./src/printaeu.d.ts ./README.md",
    "build:windows": "node .\\node_modules\\typescript\\bin\\tsc .\\src\\printaeu.ts --declaration && copy .\\src\\printaeu.d.ts .\\dist\\ && copy .\\README.md .\\dist\\",
    "pack": "webpack --mode production",
    "pub:linux": "npm publish ./dist --access=public",
    "pub:windows": "npm publish .\\dist\\ --access=public",
    "test": "jest"
  },
  "keywords": [
    "print",
    "console",
    "time",
    "stamp",
    "inline",
    "colors",
    "timestamp",
    "replace",
    "line"
  ],
  "author": "Planet's Lightning Arrester",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/planetsLightningArrester/printaeu/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "engineStrict": true,
  "homepage": "hhttps://github.com/planetsLightningArrester/printaeu#readme",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.12",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/webpack-node-externals": "^2.5.3",
    "babel-jest": "^29.3.1",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^29.3.1",
    "run-script-os": "^1.1.6",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
